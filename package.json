    "@tiptap/extension-code": "^2.6.6",
    "@tiptap/extension-code-block": "^2.6.6",
    "@tiptap/extension-color": "^2.6.6",
    "@tiptap/extension-document": "^2.6.6",
    "@tiptap/extension-dropcursor": "^2.6.6",
    "@tiptap/extension-gapcursor": "^2.6.6",
