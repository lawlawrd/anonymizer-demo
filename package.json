{
  "type": "module",
  "name": "anonymizer",
  "version": "0.0.1",
  "description": "Anonymize texts, using Presidio, for Law Law BV",
  "main": "anonymizer.js",
  "scripts": {
    "start": "node anonymizer.js",
    "dev": "concurrently \"sass styles/light.scss public/anonymizer-light.min.css --watch --style=compressed --no-source-map --quiet-deps\" \"sass styles/dark.scss public/anonymizer-dark.min.css --watch --style=compressed --no-source-map --quiet-deps\" \"nodemon anonymizer.js -e js,pug,css --ignore 'public/images' --watch\" \"webpack --watch --config webpack.prod.js\"",
    "build": "concurrently \"sass styles/light.scss public/anonymizer-light.min.css --style=compressed --no-source-map --quiet-deps\" \"sass styles/dark.scss public/anonymizer-dark.min.css --style=compressed --no-source-map --quiet-deps\"  \"webpack --config webpack.prod.js\""
  },
  "author": "Ben van den Ende",
  "license": "MIT",
  "dependencies": {
    "@tiptap/core": "^2.6.6",
    "@tiptap/extension-blockquote": "^2.6.6",
